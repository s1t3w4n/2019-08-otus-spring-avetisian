<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="questionsDao" class="dao.QuestionsDaoImpl">
        <constructor-arg name="filePath" value="src/main/resources/questions.csv"/>
    </bean>

    <bean id="consoleService" class="console.ConsoleServiceImpl">
        <constructor-arg name="questionsDao" ref="questionsDao"/>
    </bean>

</beans>